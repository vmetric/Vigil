<Window x:Class="Vigil.LiveMap"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Vigil"
        mc:Ignorable="d"
        Title="LiveMap" Height="720" Width="1280">
    <Grid>
        <Label Panel.ZIndex="1" x:Name="Label_Location" Content="-location not yet received-" HorizontalAlignment="Center" VerticalAlignment="Top" Height="45" Width="290" FontSize="24" Margin="0,79,0,0"/>
        <Image Panel.ZIndex="0" HorizontalAlignment="Left" VerticalAlignment="Top" Stretch="UniformToFill" Source="/Images/Outline1.png"/>

        <Canvas x:Name="Canvas_PinCanvas" Panel.ZIndex="1" Height="720" Width="1280" HorizontalAlignment="Left" VerticalAlignment="Top">
            <Image x:Name="Image_Pin" Panel.ZIndex="2" Height="50" Width="50" Canvas.Left="0" Canvas.Top="0" Source="/Images/ic_find3-playstore.png"/>
        </Canvas>


    </Grid>
</Window>
